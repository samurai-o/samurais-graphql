{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@app/configuration": [
        "libs/configuration/src"
      ],
      "@app/configuration/*": [
        "libs/configuration/src/*"
      ],
      "@app/logger": [
        "libs/logger/src"
      ],
      "@app/logger/*": [
        "libs/logger/src/*"
      ],
      "@app/email": [
        "libs/email/src"
      ],
      "@app/email/*": [
        "libs/email/src/*"
      ],
      "@app/datasource": [
        "libs/datasource/src"
      ],
      "@app/datasource/*": [
        "libs/datasource/src/*"
      ],
      "@app/models": [
        "libs/models/src"
      ],
      "@app/models/*": [
        "libs/models/src/*"
      ],
      "@app/prismastore": [
        "libs/prismastore/src"
      ],
      "@app/prismastore/*": [
        "libs/prismastore/src/*"
      ],
      "@app/decorators": [
        "libs/decorators/src"
      ],
      "@app/decorators/*": [
        "libs/decorators/src/*"
      ],
      "@app/middlewares": [
        "libs/middlewares/src"
      ],
      "@app/middlewares/*": [
        "libs/middlewares/src/*"
      ],
      "@app/pipes": [
        "libs/pipes/src"
      ],
      "@app/pipes/*": [
        "libs/pipes/src/*"
      ],
      "@app/filters": [
        "libs/filters/src"
      ],
      "@app/filters/*": [
        "libs/filters/src/*"
      ],
      "@app/exceptions": [
        "libs/exceptions/src"
      ],
      "@app/exceptions/*": [
        "libs/exceptions/src/*"
      ],
      "@app/interceptors": [
        "libs/interceptors/src"
      ],
      "@app/interceptors/*": [
        "libs/interceptors/src/*"
      ]
    }
  }
}